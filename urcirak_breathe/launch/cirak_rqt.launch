<?xml version="1.1"?>
<launch>

  <!-- Load RQT with a pre-setup GUI for Baxter controls from a perspective file  -->
  <node name="cirak_rqt" pkg="rqt_gui" type="rqt_gui" respawn="false"
	output="screen" args="--perspective-file $(find urcirak_breathe)/launch/cirak_is_breathing.perspective"/>

</launch>
